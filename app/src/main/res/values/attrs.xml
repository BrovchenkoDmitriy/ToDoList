<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TaskView">
        <attr name="taskName" format="string" />
        <attr name="taskDateStart" format="string" />
        <attr name="startTime" format="float" />
        <attr name="endTime" format="float" />
    </declare-styleable>
    <declare-styleable name="CustomCalendarGroup">
        <attr name="numberOfRows" format="integer"/>
        <attr name="eachRowHeight" format="dimension"/>
        <attr name="minimumPercentage" format="float"/>
        <attr name="dividerStartOffset" format="dimension"/>
    </declare-styleable>
</resources>